{
  "identity": "user",
  "connection": "default",
  "schema": true,
  "limit": 20,
  "defaultTemplate": "default",
  "displayedAttribute": "username",
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "unique": true
    },
    "lang": {
      "type": "string",
      "required": true
    },
    "template": {
      "type": "string",
      "required": true
    },
    "id_ref": {
      "type": "int"
    },
    "provider": {
      "type": "string"
    },
    "password": {
      "type": "string",
      "minLength": 4
    },
    "resetPasswordToken": {
      "type": "string"
    },
    "roles": {
      "collection": "role",
      "via": "users",
      "dominant": true
    },
    "products": {
      "collection": "product",
      "via": "contributors"
    },
    "producttypes": {
      "collection": "producttype",
      "via": "contributors"
    },
    "productparams": {
      "collection": "productparam",
      "via": "contributors"
    },
    "producttypedefaultparams": {
      "collection": "producttypedefaultparam",
      "via": "contributors"
    },
    "tests": {
      "collection": "test",
      "via": "contributors"
    },
    "productprices": {
      "collection": "productprice",
      "via": "contributors"
    }
  },
  "autoCreatedAt": true,
  "autoUpdatedAt": true,
  "migrate": "alter",
  "templates": {
    "default": {
      "attributes": {
        "username": {
          "type": "string",
          "minLength": 3,
          "unique": true
        },
        "email": {
          "type": "email",
          "minLength": 6,
          "unique": true
        }
      },
      "displayedAttribute": "username"
    }
  }
}
